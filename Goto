BEGIN
GOTO header_message;
<<greeting_message>>
DBMS_OUTPUT.PUT_LINE( 'Hi' );
GOTO ending_message;
<<header_message>>
DBMS_OUTPUT.PUT_LINE( ' Demonstration of working of PL/ SQL GOTO statement ' );
GOTO greeting_message;
<<ending_message>>
DBMS_OUTPUT.PUT_LINE( 'Wish you all the best for your learnings...' );
end;
